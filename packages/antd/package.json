{
    "name": "@stbui/prophet-antd",
    "version": "3.0.1",
    "description": "用于构建中后台前端应用框架",
    "main": "lib/index",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "homepage": "https://github.com/stbui/prophet",
    "bugs": "https://github.com/stbui/prophet/issues",
    "author": "stbui",
    "license": "ISC",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
