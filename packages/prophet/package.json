{
    "name": "@stbui/prophet",
    "version": "3.0.1",
    "description": "用于构建中后台前端应用框架",
    "main": "lib/index",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "rimraf ./lib && tsc --watch"
    },
    "peerDependencies": {
        "react": "^16.9.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.9.0 || ^17.0.0 || ^18.0.0",
        "react-router": "^6.1.0",
        "react-router-dom": "^6.1.0"
    },
    "dependencies": {
        "@stbui/prophet-antd": "^3.0.1",
        "@stbui/prophet-core": "^3.0.1",
        "@stbui/prophet-i18n-polyglot": "^3.0.1",
        "@stbui/prophet-language-chinese": "3.0.0",
        "antd": "^4.23.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.1.0",
        "react-router-dom": "^6.1.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "author": "stbui",
    "homepage": "https://github.com/stbui/prophet#readme",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stbui/prophet.git"
    },
    "bugs": {
        "url": "https://github.com/stbui/prophet/issues"
    }
}
