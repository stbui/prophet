{
  "name": "react-admin-kit",
  "version": "1.0.0",
  "homepage": "https://github.com/stbui/react-admin-kit",
  "bugs": "https://github.com/stbui/react-admin-kit/issues",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build",
    "postinstall": "lerna bootstrap",
    "release": "lerna publish --exact --conventional-commits",
    "demo": "cd examples/demo && npm run start",
    "start": "cd examples/starter && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "cd docs && gitbook build",
    "start:docs": "cd docs && gitbook serve",
    "publish:github": "gh-pages -d docs/_book",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "stbui",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "lerna": "^3.7.2",
    "typescript": "^3.2.2",
    "conventional-changelog-cli": "^2.0.11"
  }
}
