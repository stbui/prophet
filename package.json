{
  "name": "prophet",
  "version": "1.0.0",
  "homepage": "https://github.com/stbui/prophet",
  "bugs": "https://github.com/stbui/prophet/issues",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build",
    "postinstall": "lerna bootstrap",
    "release": "npm run build && lerna publish --exact --conventional-commits",
    "demo": "cd examples/demo && npm run start",
    "start": "cd examples/starter && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "cd docs && gitbook build",
    "prettier": "prettier --config ./.prettierrc.js --write --list-different \"packages/*/src/**/*.{js,json,ts,tsx}\"",
    "start:docs": "cd docs && gitbook serve",
    "publish:github": "gh-pages -d docs/_book",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint --ext .js,.ts,.tsx \"./packages/**/src/**/*.{js,ts,tsx}\""
  },
  "author": "stbui",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "typescript": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "conventional-changelog-cli": "^2.0.11"
  }
}
